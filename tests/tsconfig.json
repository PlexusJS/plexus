{
	"extends": "./../packages/tsconfig.shared.json",
	"compilerOptions": {
		"target": "ES2016",
		"lib": ["es5", "es6", "es2017", "ES2018", "dom"],
		"jsx": "react",
		"types": ["vitest/globals", "bun-types"],
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"noImplicitThis": true,
		"rootDir": ".",
		"module": "CommonJS",
		"declaration": true,
		"declarationMap": true,
		"strict": true,
		"composite": true,
		"noImplicitAny": false,
		"skipLibCheck": true
	}
}
