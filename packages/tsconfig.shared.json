{
	"compilerOptions": {
		"target": "ES2016",
		"lib": ["es5", "es6", "es2017", "ES2018", "dom"],
		"module": "CommonJS",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"strict": true,
		"composite": true,
		"noImplicitAny": false,
		"noImplicitThis": true,
		"skipLibCheck": true,
		"esModuleInterop": true
	}
}
