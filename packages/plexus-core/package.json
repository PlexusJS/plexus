{
	"name": "@plexusjs/core",
	"version": "1.10.9",
	"description": "A library for building modular interconnected applications",
	"main": "./dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"plexusjs",
		"plexus",
		"plexus-core",
		"react",
		"reactjs",
		"global state",
		"shared state",
		"plugin"
	],
