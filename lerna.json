{
	"version": "1.8.4",
	"npmClient": "yarn",
	"useNx": true,
	"packages": ["packages/*"],
	"command": {
		"version": {
			"exact": true
		},
		"publish": {
			"npmClient": "yarn",
			"allowBranch": ["master", "canary"],
			"registry": "https://registry.npmjs.org/",
			"ignoreChanges": ["docs/**/*"]
		}
	}
}
